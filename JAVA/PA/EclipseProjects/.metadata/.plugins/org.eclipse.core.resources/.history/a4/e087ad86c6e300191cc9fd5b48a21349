
class Automovel {

	// Attributes
	String paint;
	String plate;
	int km;
	int doorNumber;
	
	// Methods
	
	// Construct
	public Automovel(String c, String m, int doorNumber, int km){
		paint = new paint;
		plate = new plate;
		km = new km;
		doorNumber = new doorNumber;
	}
	
	public static changePaint(String newPaint)
	{
		paint = newPaint;
	}
}
