
class Automovel {

	// Attributes
	String paint;
	String plate;
	int doorNumber;
	double km;
	
	// Methods
	
	// Construct
	public Automovel(String paint, String pl, int dNumber, int km){
		paint = paint;
		plate = pl;
		doorNumber = dNumber;
		this.km = km;
	}
	
	public static changePaint(String newPaint)
	{
		paint = newPaint;
	}
}
