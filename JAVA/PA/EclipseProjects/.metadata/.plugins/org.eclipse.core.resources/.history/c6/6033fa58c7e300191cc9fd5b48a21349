
class Automovel {

	// Attributes
	String paint;
	String plate;
	int doorNumber;
	double km;
	
	// Methods
	
	// Construct, attributes with same name require "this."
	public Automovel(String paint, String plate, int doorNumber, int km){
		this.paint = paint;
		this.plate = plate;
		this.doorNumber = doorNumber;
		this.km = km;
	}
	
	public static changePaint(String newPaint)
	{
		paint = newPaint;
	}
}
